    "next": "^9.4.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.11.0",
    "react-id-swiper": "^4.0.0",
    "react-swipeable-views": "^0.13.9",
    "styled-components": "^5.0.0",
